<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.korinver.androidsivar">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/android"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/android_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.korinver.androidsivar.DataRecordDetail"></activity>
        <activity android:name="com.korinver.androidsivar.DetailActivity" />
        <activity android:name="com.korinver.androidsivar.MainActivity"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>